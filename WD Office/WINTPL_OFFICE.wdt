      // Summary: <specify the action of the procedure>
      // Syntax:
      //MenuListLibraries ()
      //
      // Parameters:
      //	None
      
      // Return value:
      procedure MenuListLibraries()
      
      
      HourGlass(True)
      
      // Disables the display
      LOOP_LIBRARY.DisplayEnabled	= False
      
      // Signals the start of fill
      LOOP_LIBRARY.StartFilling()
      
      // Clears the list
      LOOP_LIBRARY.DeleteAll()
      
      for each stElement of Library
      	
      	sType is string
      	if stElement.Custom = False then
      		sType = "STANDARD"
      	else
      		sType = "CUSTOM"
      		// If the document does not exist anymore, ignore it
      		if fFileExist(stElement.FullPath) = False then continue
      	end
      	
      	// Loads the details of the document				
      	nRow	is int		= LOOP_LIBRARY.AddLine(stElement.Custom, sType, stElement.Caption, stElement.ToLoad,stElement.FullPath)
      	
      	
      end
      
      // Signals the end of fill
      LOOP_LIBRARY.EndFilling()
      LOOP_LIBRARY.SelectMinus()
      
      LOOP_LIBRARY.DisplayEnabled	= True
      
      HourGlass(False)
