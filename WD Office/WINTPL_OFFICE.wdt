         // Applies the background color to the menu elements
         LAYOUT_MENU.BackgroundColor					= MySelf.BackgroundColor
         IMG_BCKGRD_MENU_BAR.BackgroundColor			= MySelf.BackgroundColor
         IMG_WINDOW_FRAME.Border.Color				= MySelf.BackgroundColor
