      // Summary: Stores the addition of a library
      // Syntax:
      //LibraryConfigAdd (<sName> is string [, <nPos> is numeric [, <bCustom> is boolean [, <bLoad> is boolean [, <sPath> is string]]]])
      //
      // Parameters:
      //	sName (ANSI string): Name of the library
      //	nPos (numeric - default value=-1): Position in the library (in the panel)
      //	bCustom (boolean - default value=0): Determines whether the library is a standard or custom library
      //	bLoad (boolean - default value=1): Determines whether or not the library must be displayed
      //	sPath (ANSI string - default value=""): Path of the .wddiag file if a custom library is used
      
      // Return value:
      procedure LibraryConfigAdd(local sName is string, local nPos is numeric = -1, local bCustom is boolean = False, local bLoad is boolean = True, local sPath is string = "")
      
      stElement	is LibrayElement
      
      // Creates an entry
      stElement.Caption			= sName
      stElement.Index				= nPos
      stElement.Custom			= bCustom
      stElement.ToLoad			= bLoad
      stElement.FullPath			= sPath
      
      // Checks if this library is already in the list
      nIndex is int = Seek(Library, asLinear + tccIgnoreCase, "Caption", sName)
      
      if nIndex > 0 then
      	// Update
      	Library[ nIndex ] = stElement
      else
      	// Add
      	Library.Add(stElement)		
      end
      
      // Saves
      LibraryConfigSave()
