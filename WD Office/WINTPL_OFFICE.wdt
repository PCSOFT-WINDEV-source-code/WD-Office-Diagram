  -
    name : BTN_FILE_MENU
    identifier : 0x2259645938899bee
    internal_properties : 
    properties :
     x : 1
     y : 36
     width : 80
     height : 31
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         
         // The [File] button must be positioned above everything
         BTN_FILE_MENU.ZOrder = 1000
        type : 14
      -
        code : |1-
         
         ///////////////////
         // Displays the menu
         ///////////////////
         
         // Stores the current plane and the properties that will be modified
         nPlane				is int				= MyWindow.Plane	
         sSysImgClose		is string			= BTN_SYST_CLOSE.Image
         sSysImgRestore		is string			= BTN_SYST_RESTORE.Image
         sSysImgMaximize		is string			= BTN_SYST_MAXIMIZE.Image
         sSysImgMinimize		is string			= BTN_SYST_MINIMIZE.Image		
         sSysBkImgClose		is string			= BTN_SYST_CLOSE.BackgroundImage
         sSysBkImgRestore	is string			= BTN_SYST_RESTORE.BackgroundImage
         sSysBkImgMaximize	is string			= BTN_SYST_MAXIMIZE.BackgroundImage
         sSysBkImgMinimize	is string			= BTN_SYST_MINIMIZE.BackgroundImage		
         
         // Prepares the display
         AnimationPrepare(MyWindow)
         
         // Modifies the properties
         BTN_SYST_CLOSE.Image				= Replace((BTN_SYST_CLOSE.Image), "_light", "")
         BTN_SYST_RESTORE.Image				= Replace((BTN_SYST_RESTORE.Image), "_light", "")
         BTN_SYST_MAXIMIZE.Image				= Replace((BTN_SYST_MAXIMIZE.Image), "_light", "")
         BTN_SYST_MINIMIZE.Image				= Replace((BTN_SYST_MINIMIZE.Image), "_light", "")
         BTN_SYST_CLOSE.BackgroundImage		= Replace((BTN_SYST_CLOSE.BackgroundImage), "_light", "")
         BTN_SYST_RESTORE.BackgroundImage	= Replace((BTN_SYST_RESTORE.BackgroundImage), "_light", "")
         BTN_SYST_MAXIMIZE.BackgroundImage	= Replace((BTN_SYST_MAXIMIZE.BackgroundImage), "_light", "")
         BTN_SYST_MINIMIZE.BackgroundImage	= Replace((BTN_SYST_MINIMIZE.BackgroundImage), "_light", "")
         IMG_WINDEV_LOGO.Visible				= False
         LAYOUT_SHORTCUTS.Visible			= False
         BTN_FILE_MENU.Visible				= False
         RADTITLE.Color						= DarkGray
         
         // Displays the menu plane, on [Information]
         MyWindow.Plane						= PLANE_INFO
         MenuMarkActive( BTN_MENU_INFO )
         
         // The click on [BACK] button must restore the modified properties
         BTN_MENU_BACK..Process[ trtClick ] = ___HideMenu
         
         // Triggers the animation
         AnimationPlay(animCoverRight, 100ms)
         
         	INTERNAL PROCEDURE ___HideMenu( bAnimation is boolean = True)
         		
         		// Prepares the display
         		if bAnimation then
         			AnimationPrepare(MyWindow)
         		else
         			MyWindow.DisplayEnabled = False
         		end
         		
         		// Restores the modified properties
         		BTN_SYST_CLOSE.Image				= sSysImgClose
         		BTN_SYST_RESTORE.Image				= sSysImgRestore
         		BTN_SYST_MAXIMIZE.Image				= sSysImgMaximize
         		BTN_SYST_MINIMIZE.Image				= sSysImgMinimize
         		BTN_SYST_CLOSE.BackgroundImage		= sSysBkImgClose
         		BTN_SYST_RESTORE.BackgroundImage	= sSysBkImgRestore
         		BTN_SYST_MAXIMIZE.BackgroundImage	= sSysBkImgMaximize
         		BTN_SYST_MINIMIZE.BackgroundImage	= sSysBkImgMinimize
         		RADTITLE.Color						= White
         		IMG_WINDEV_LOGO.Visible				= True
         		LAYOUT_SHORTCUTS.Visible			= True
         		BTN_FILE_MENU.Visible				= True
         		
         		BTN_MENU_OPTIONS.Pushed				= False
         		BTN_MENU_EXPORT.Pushed				= False
         		BTN_MENU_PRINT.Pushed				= False
         		BTN_MENU_SAVE_AS.Pushed				= False
         		BTN_MENU_OPEN.Pushed				= False
         		BTN_MENU_NEW.Pushed					= False
         		BTN_MENU_INFO.Pushed				= False
         		
         		// Restores the initial plane
         		MyWindow.Plane						= nPlane
         		
         		// Triggers the animation
         		if bAnimation then
         			AnimationPlay(animCoverLeft, 100ms)
         		else
         			MyWindow.DisplayEnabled = True		
         		end
         		
         		// Cancels the click process
         		BTN_MENU_BACK..Process[ trtClick ] = Null
         	end
        type : 18
      -
        type : 161
        enabled : false
