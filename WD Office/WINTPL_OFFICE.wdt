         LOOP_RECENT_LOCATIONS.STC_PERIOD_NAME.Color	= MySelf.BackgroundColor
         
         // Manages the double click
         MySelf..Process[ trtDoubleClick ]			= ___OnDoubleClickTitleBar
         	INTERNAL PROCEDURE ___OnDoubleClickTitleBar
         		
         		// If the window is maximized
         		if WinSize() = MaxSize then
         			
         			// Restores the standard window size
         			Restore()
         		else
         			
         			// Maximizes the window
         			Maximize()
         		end
         		
         	end
