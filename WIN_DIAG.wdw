      // Summary : Initialize the "Library" panel
      // Syntax:
      //InitializeLibraryPanel ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      procedure InitializeLibraryPanel()
      
      //Disable the load and add library button usually available in the "Library" panel
      //Since it is customized here
      if ControlExist("BTN_OpenLibraryMenu") = True then
      	// Show the option
      	extern BTN_OpenLibraryMenu
      	BTN_OpenLibraryMenu..Visible = False
      end
      
      //Stores the standard libraries in memory to display them again if needed
      gStandardLibrary = DIAGEDT_DIAGRAM.Library
      
      //JSON file with the standard and custom libraries to be displayed in the "Library" panel
      sLibraryFile	is string	= fDataDirUser() + [fSep] + "library.json"
      
      // Checks the existence of the JSON file
      if fFileExist(sLibraryFile) = False then
      	
      	//If the JSON file does not exist yet (e.g., first launch)
      	//Create it by listing the standard libraries. They are all displayed by default
      	FOR EACH ELEMENT stLibraryElement,i of DIAGEDT_DIAGRAM.Library
      		LibraryConfigAdd(stLibraryElement.name, i, False )
      	end
      	LibraryConfigSave()
      	
      else
      	
      	__UpdateLibraryPanel()
      	
      end
      
