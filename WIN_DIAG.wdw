     code : |1-
      // Summary: Initializes the custom ribbon (mandatory after the initialization code of window)
      // Syntax:
      //InitializeRibbon (<sSourceRibbon> is string [, <oCustomRibbon> is control [, <pHideCallbackRibbon> is procedure]]) 
      //
      // Parameters:
      //	sSourceRibbon (ANSI string): Name of native ribbon
      //	oCustomRibbon (control - default value=0): Custom ribbon control
      //	pHideCallbackRibbon (procedure - default value=0): Custom callback for hiding the ribbon
      
      // Return value:
      procedure InitializeRibbon(sSourceRibbon is string, oCustomRibbon is Control = Null, pHideCallbackRibbon is procedure = Null)
      
      // Makes the window visible
      gDocumentWin..Visible = True
      
      // Checks the existence of source ribbon control
      if ControlExist(sSourceRibbon) = False then
      	
      	// It does not exist: it was customized or it is the standard toolbar, ignore
      	return
      end
      
      oNativeRibbon	is champ	<- {sSourceRibbon, indControl}
      
      
      // Applies the same style as the window
      //Code not required here because the Diagram Editor ribbon does not have a "visible" tab
      //	oNativeRibbon..Style 	= RIBBON_STYLE..Style
      //	oNativeRibbon..Image 	= gsImageRibbonTab
      
      // Masks the first ribbon group (its features correspond to the file menu managed by the template)
      sGroupName		is string	= EnumControl(oNativeRibbon[1]..FullName, 1)
      if sGroupName <> "" then {sGroupName, indControl}..Visible = False
      
      // If a custom callback was supplied for hiding the ribbon
      if pHideCallbackRibbon <> Null then
      	// Calls the custom callback for hiding the ribbon
      	pHideCallbackRibbon( oNativeRibbon )
      end
      
      // Changes the background color
      oColor is Color = STC_BCKGRD_TITLE_BAR.BackgroundColor
      oNativeRibbon..BackgroundColor = oColor
      
      // Generates the 5-state background image for the buttons
      with oColor
      	
      	oButtonImageStyle is Image
      	oButtonImageStyle.Width		= 100
      	oButtonImageStyle.Height	= 20
      	
      	// Start of drawing
      	dStartDrawing(oButtonImageStyle, dWithOpacity)
      	
      	// Pressed
      	oColor = STC_BCKGRD_TITLE_BAR.BackgroundColor
      	..Saturation = 180
      	..Luminance = 95
      	..Opacity = 50
      	
      	dBackground(..Color, BackgroundSolid, 0, ..Opacity)
      	dPen(Transparent)
      	dRectangle(20, 0, 39, 19)
      	
      	// Active
      	oColor = STC_BCKGRD_TITLE_BAR.BackgroundColor
      	..Saturation = 180
      	..Luminance = 115
      	..Opacity = 50
      	
      	dBackground(..Color, BackgroundSolid, 0, ..Opacity)
      	dPen(Transparent)
      	dRectangle(60, 0, 79, 19)
      	
      	// Hover
      	oColor = STC_BCKGRD_TITLE_BAR.BackgroundColor
      	..Saturation = 30
      	..Luminance = 160
      	..Opacity = 40
      	
      	dBackground(..Color, BackgroundSolid, 0, ..Opacity)
      	dPen(Transparent)
      	dRectangle(80, 0, 99, 19)		
      	
      	// Save
      	gsButtonImageStyle = fDataDirUser() + [fSep] + "btn5.png"
      	dSaveImagePNG(oButtonImageStyle, gsButtonImageStyle)
      end
      
      // Applies the generated image to the declared buttons
      for each oControl of garrControlsStyle
      	if oControl = Null then continue
      	
      	// Applies the style
      	__ApplyStyle(oControl)
      end
      
      // Inserts the custom tabs into the native ribbon: used to keep the updates of "native" ribbon
      if oCustomRibbon <> Null then
      	RibbonDuplicateTabs(oCustomRibbon, oNativeRibbon)	
      end
      
      // Moves the UNDO/REDO buttons
      MoveUndoRedo()
      
      // Moves the read-only caption
      MoveReadOnly()
      
      end:	
      // The ribbon is updated every 250ms
      TimerSys(OnRibbonUpdate, 250ms)
