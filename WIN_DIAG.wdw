     name : LibraryConfigAdd
     internal_properties : 
     procedure_id : 1322219670864295624
     type_code : 14
     code : |1-
      // Summary: Stores the addition of a library
      // Syntax:
      //LibraryConfigAdd (<sName> is string [, <nPos> is numeric [, <bCustom> is boolean [, <bLoad> is boolean [, <sPath> is string]]]])
      //
      // Parameters:
      //	sName (ANSI string): Name of the library
      //	nPos (numeric - default value=-1): Position in the library (in the panel)
      //	bCustom (boolean - default value=0): Determines whether the library is a standard or custom library
      //	bLoad (boolean - default value=1): Determines whether or not the library must be displayed
      //	sPath (ANSI string - default value=""): Path of the .wddiag file if a custom library is used
      
      // Return value:
      procedure LibraryConfigAdd(local sName is string, local nPos is numeric = -1, local bCustom is boolean = False, local bLoad is boolean = True, local sPath is string = "")
      
      stElement	is LibrayElement
      
      // Creates an entry
      stElement.Caption			= sName
      stElement.Index				= nPos
      stElement.Custom			= bCustom
      stElement.ToLoad			= bLoad
      stElement.FullPath			= sPath
      
      // Checks if this library is already in the list
      nIndex is int = Seek(Library, asLinear + tccIgnoreCase, "Caption", sName)
      
      if nIndex > 0 then
      	// Update
      	Library[ nIndex ] = stElement
      else
      	// Add
      	Library.Add(stElement)		
      end
      
      // Saves
      LibraryConfigSave()
     type : 458752
   -
     name : LibraryConfigLoad
     internal_properties : 
     procedure_id : 1322219670864361160
     type_code : 14
     code : |1-
      // Summary: Loads the list of libraries to be displayed
      // Syntax:
      //[ <Result> = ] LibraryConfigLoad ()
      //
      // Parameters:
      //	None
      // Automatic exception processing:   run the block of code CASE EXCEPTION:
      //
      
      // Return value:
      // 	array (LibraryElement): List of libraries
      // Automatically process exceptions:   run CASE EXCEPTION:
      procedure LibraryConfigLoad() : array of LibrayElement
      
      arrLibrary		is array of LibrayElement
      sJSON			is string ANSI
      sLibraryFile	is string	= fDataDirUser() + [fSep] + "library.json"
      
      // Checks the file existence
      if fFileExist(sLibraryFile) = False then
      	return arrLibrary
      end
      
      // Loads the data
      sJSON = fLoadText(sLibraryFile)
      
      // Empty backup
      if sJSON = "" then
      	return arrLibrary
      end
      
      // Deserializes the content
      Deserialize(arrLibrary, sJSON, psdJSON)
      
      // Copies the list again
      return arrLibrary
      
      case exception:
      return arrLibrary
     type : 458752
