     name : MenuCloseDocument
     internal_properties : CAAAAAgAAABlOl5l4rFdBtr+sud6fSJn0baH7o2/PBEfZVPgA6a+tGbAUUqltZJezv/X5UYPTk7JOOr6EDb5+vc9hFKFH66lJiyvYcfio/qokOXqKjLfwerjS8dzd7saRRJdazC0+HJ54Fd//RBG/40OSDxS0QI3C0N+puQ1PP8I5WMpA+oAcfdF7+2YnpbrfDkqInygIQfRxUZlngj7xiHO24P8gTfiCCByOzoRP9i2uEeZZYb0zGyDl4OCfT2zx7UndmADLP5cNFzrtkiffoBQdNylGSop
     procedure_id : 1322219670863640264
     type_code : 14
     code : |1-
      // Summary: Closes the current document
      // Syntax:
      //[ <Result> = ] MenuCloseDocument ( [<bCloseWindow> is boolean])
      //
      // Parameters:
      //	bCloseWindow (boolean - default value=1): True to force the window closing, False otherwise
      // Automatic exception processing:   run the block of code CASE EXCEPTION:
      //
      
      // Return value:
      //	boolean: True if the closing was successfully performed, False otherwise
      // Automatically process exceptions:   run CASE EXCEPTION:
      procedure MenuCloseDocument(local bCloseWindow is boolean = True) : boolean
      
      // If the document was modified
      if gbModifiedDocument then
      	
      	//1: Save
      	//2: Don't save
      	//3: Cancel
      	switch Dialog(<§?128C491E00466BE7§>) 
      	
      		// Save
      		case 1 
      			
      			// Saves the document
      			if MenuSaveDocument() = -1 then
      				return False
      			end
      			
      		// Don't save
      		case 2
      			
      			// No action
      			
      		// Cancel
      		case 3
      			// Stop everything and resume
      			return False
      	end
      end
      
      // Triggers the closing
      if DocumentAction.CloseDocument = Null _or_ DocumentAction.CloseDocument( gsDocumentPath ) = True then
      	
      	// Stores the document in the MRU
      	MRUAdd( gsDocumentPath, gxDocPosition )
      	
      	// Closes
      	if bCloseWindow then Close()	
      	return True
      end
      
      return False
      
      case exception:
      dbgAssert(False, ExceptionInfo)
      if bCloseWindow then Close()
      return False
