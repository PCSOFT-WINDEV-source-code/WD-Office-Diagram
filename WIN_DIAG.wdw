     code : |1-
      // Summary: Refreshes the libraries in the panel
      // Syntax:
      //__UpdateLibraryPanel ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      procedure __UpdateLibraryPanel()
      
      //Load the Library panel configuration
      arrLibrary is array of LibrayElement
      arrLibrary = LibraryConfigLoad()
      
      //Start by deleting all libraries 
      //New libraries will be added as required
      ArrayDeleteAll(DIAGEDT_DIAGRAM.Library)
      
      //Add standard libraries first 
      for each stLibraryElement of arrLibrary where custom = False
      	if stLibraryElement.ToLoad = True then
      		Add(DIAGEDT_DIAGRAM.Library,gStandardLibrary.Library[stLibraryElement.Index])
      	end
      end
      
      //Then add custom libraries 
      for each stLibraryElement of arrLibrary where custom = True
      	if stLibraryElement.ToLoad = True then
      		
      		CustomLibrary is diagLibrary
      		CustomLibrary.Name = stLibraryElement.Caption
      		// If the document that corresponds to the custom library no longer exists, ignore it
      		if fFileExist(stLibraryElement.FullPath) = False then continue
      		//Loads the custom library .wddiag file 
      		DiagramLoadLibrary(CustomLibrary,stLibraryElement.FullPath)
      		//and adds it to the panel
      		Add(DIAGEDT_DIAGRAM.Library, CustomLibrary)
      		
      	end
      end
