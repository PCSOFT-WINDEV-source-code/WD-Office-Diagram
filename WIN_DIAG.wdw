     name : AddNewLibrary
     internal_properties : 
     procedure_id : 1322219670864230088
     type_code : 14
     code : |1-
      // Summary: Adds a new library to the Diagram Editor control
      // Syntax:
      //[ <Result> = ] AddNewLibrary (<sName> is string, <sDirectory> is string)
      //
      // Parameters:
      //	sName (ANSI string): Name to assign to the library
      //	sDirectory (ANSI string): Directory containing svg files
      
      // Return value:
      // 	ANSI string: .wddiag file saved 
      procedure AddNewLibrary(sName is string, sDirectory is string )
      
      // New library to create
      MyLibrary is diagLibrary
      MyLibrary.Name = sName
      
      // Lists .svg files in the "library" directory
      sListSVG is string
      sListSVG = fListFile(sDirectory + fSep() +"*.svg", frNotRecursive)
      
      // For each file
      for each string sAPath of sListSVG SÉPARÉE by CR
      	
      	// Creates a diagImage object
      	MyImage is diagImage
      	
      	// Stores the full path of the source SVG file
      	MyImage.Image		= sAPath
      	
      	// For the name, it is possible to use:
      	// - information contained in the SVG file (e.g., content of the <title> tag)
      	MyImage.Name		= GetTitleTag(sAPath)
      	// - the name of the file (or part of the name)
      	//MyImage.Name		= ExtractString(fExctractPath(sAPath, fFileName), 2, "-")
      	
      	// Ability to force a default size for the object when added to the diagram
      	MyImage.Width		= 120
      	MyImage.Height		= 120
      	
      	// Adds this image to the library
      	MyLibrary.Forme.Ajoute(MyImage)
      	
      end
      
      sFile is string = fDataDirUser(fddDefault) + fSep() + sName + ".wddiag"
      
      DiagramSave(MyLibrary, sFile)
      
      
      	INTERNAL PROCEDURE GetTitleTag(sSVGPath is string)
      		
      		// Loads the content of the SVG file into an xmlDocument
      		MySVG is xmlDocument = XMLOpen(sSVGPath)
      		
      		// Retrieves the content of the title tag
      		return MySVG.svg.title
      		
      	end
      
      return sFile
     type : 458752
